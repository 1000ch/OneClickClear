{
	"name": "OneClickClear",
	"version": "0.3.1",
	"manifest_version": 2,
	"description": "This extension provides an easy way to clear browser history.",
	"permissions": ["history", "*://*/*"],
	"background": {
		"scripts": ["/src/js/background.js"]
	},
	"browser_action": {
		"default_icon": "/src/icon/icon.png",
		"default_title": "OneClickClear"
	}
}